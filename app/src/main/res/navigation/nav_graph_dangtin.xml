    </fragment>
    <fragment
        android:id="@+id/navigation_ketquagiaodich_dangtin"
        android:name="Fg_ThanhToan.KetQuaGiaoDich_DangTinFg"
        tools:layout="@layout/fg_ketquagiaodich_dangtin"
        android:label="Phương thức thanh toán"/>
</navigation>