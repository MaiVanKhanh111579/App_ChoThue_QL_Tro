        android:label="Tài khoản">
        <action
            android:id="@+id/action_navigation_taikhoan_dangtin_to_navigation_qlmatkhau_dangtin"
            app:destination="@id/navigation_qlmatkhau_dangtin" />
        <action
            android:id="@+id/action_navigation_taikhoan_dangtin_to_navigation_qlthongtin_dangtin"
            app:destination="@id/navigation_qlthongtin_dangtin" />
    </fragment>

    <fragment
