        <action
            android:id="@+id/action_navigation_qltindang_to_navigation_dangtinmoi"
            app:destination="@id/navigation_dangtinmoi" />
        <action
            android:id="@+id/action_navigation_qltindang_to_navigation_qltindang_show_thongtin_tindang"
            app:destination="@id/navigation_qltindang_show_thongtin_tindang" />
    </fragment>
    <fragment
        android:id="@+id/navigation_khachhang"
        android:name="Fg_KhachHang.KhachHangFg"
